<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>End_Date_greater_than_Start_Date</fullName>
    <active>true</active>
    <errorConditionFormula>DATETIMEVALUE( Start_Date__c )  &gt; DATETIMEVALUE( End_Date__c )</errorConditionFormula>
    <errorDisplayField>End_Date__c</errorDisplayField>
    <errorMessage>Start Date should be less than End Date</errorMessage>
</ValidationRule>
