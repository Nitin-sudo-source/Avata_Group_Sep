<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Validation_for_Registration_Date_Requir</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISPICKVAL( Registration_Status__c , &apos;Registration Completed&apos;) ,ISBLANK( Registration_Date__c ))</errorConditionFormula>
    <errorMessage>Registration Date is Mandatory</errorMessage>
</ValidationRule>
