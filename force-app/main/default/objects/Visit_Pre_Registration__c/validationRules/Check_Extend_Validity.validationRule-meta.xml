<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Check_Extend_Validity</fullName>
    <active>false</active>
    <errorConditionFormula>!ISNEW() &amp;&amp; ( Extend_Validity_In_Hours__c  &lt; 48 || Extend_Validity_In_Hours__c  &gt; 48)</errorConditionFormula>
    <errorMessage>The Extend Validity value must be exactly 48 hours</errorMessage>
</ValidationRule>
